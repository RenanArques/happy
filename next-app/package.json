{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:run": "ts-node-dev -O '{\"module\":\"commonjs\"}' ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node-dev -O '{\"module\":\"commonjs\"}' ./node_modules/typeorm/cli.js migration:revert",
    "migration:create": "ts-node-dev -O '{\"module\":\"commonjs\"}' ./node_modules/typeorm/cli.js migration:create"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "formidable": "^1.2.2",
    "leaflet": "^1.7.1",
    "next": "9.5.5",
    "next-connect": "^0.9.0",
    "next-images": "^1.6.0",
    "pg": "^8.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.11.0",
    "react-leaflet": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^8.0.5",
    "typeorm": "0.2.22",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@types/cors": "^2.8.8",
